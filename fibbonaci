package com.company;
import java.util.Scanner;
public class Main{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
System.out.print("enter the value of n : "+ " ");
int n=sc.nextInt();
int c;
int a=0;
int b=1;
System.out.print(a+" "+b);
for(int i=1;i<=n;i++){
c=a+b;
System.out.print(" "+c);
a=b;
b=c;
}
}
}
